                <ColumnDefinition Width="740*" />
                <ColumnDefinition Width="8*" />
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition Height="34"/>
                <RowDefinition Height="*"/>
            </Grid.RowDefinitions>
            <Grid Background="#FF3A7ECB" ShowGridLines="False" Grid.ColumnSpan="2">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="28"/>
                    <ColumnDefinition Width="*"/>
                </Grid.ColumnDefinitions>
                <!-- alanrevisar esta linea nueva tiene un tag adicional
                <TextBlock x:Name="textblockfxOptionsTitle" Grid.Column="1" Text="Opciones FX" TextWrapping="Wrap" VerticalAlignment="Center" Foreground="#FFFFFFFF"/>
                -->
                <TextBlock Grid.Column="1" Text="Opciones FX - V.r2.09" TextWrapping="Wrap" VerticalAlignment="Center" Foreground="#FFFFFFFF"/>
            </Grid>


            <SisWinControls:TabControl x:Name="tabPrincipal" Margin="20,22,10,8"  Grid.Row="1" Width="710" Canvas.Left="10" Canvas.Top="10" SelectedIndex="0">
