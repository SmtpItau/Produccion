                            <Canvas x:Name="CanvasDefinicionOpcion" >
                                <RadioButton x:Name="radioCompra"  Height="22.545" Width="69" Canvas.Left="20" Canvas.Top="8" Content="Compra" IsChecked="True" Checked="compraChecked"/>
                                <RadioButton x:Name="radioVenta"   Height="23" Width="60" Content="Venta" Canvas.Left="93" Canvas.Top="8" Checked="ventaChecked"/>

                                <!--Plazo-->
                                <TextBlock Height="13" Width="30" Text="Plazo" Canvas.Left="20" Canvas.Top="108"/>
                                <TextBox x:Name="txtPlazo" KeyDown="event_KeyDown_IntegerText" GotFocus="event_GotFocus_IntegerText" TextChanged="event_TextChanged_IntegerText" LostFocus="event_LostFocus_IntegerText"  Height="24" Width="85" Canvas.Left="20" Canvas.Top="123" TextAlignment="Right"/>
                                <!--<TextBox Name="txtPlazo" TextChanged="txtMessageExpiryDate_TextChanged" LostFocus="txtMessageExpiryDate_LostFocus" Height="24" Width="55" Canvas.Left="20" Canvas.Top="140" TextAlignment="Right"/>-->
                                <SisWinControls:DatePicker x:Name="DatePickerVencimiento" Height="24" Width="118" Canvas.Left="110" Canvas.Top="123" GotFocus="DatePickerVencimiento_GotFocus" LostFocus="DatePickerVencimiento_LostFocus" CalendarOpened="DatePickerVencimiento_CalendarOpened" CalendarClosed="DatePickerVencimiento_CalendarClosed" KeyUp="DatePickerVencimiento_KeyUp"/>


                                <!--ComboSunyacente (Paridad)-->
                                <TextBlock Text="Subyacente" Canvas.Left="20" Canvas.Top="65" FontSize="11" Width="71" Height="16"/>
                                <ComboBox  x:Name="comboSubyacente" Height="24" Width="85" Canvas.Left="20" Canvas.Top="81" SelectedIndex="0"  IsEnabled="False"  >
                                    <ComboBoxItem Content="CLP/USD" />
                                </ComboBox>

                                <!--ComboPayoff-->
                                <TextBlock Text="Payoff" Canvas.Left="115" Canvas.Top="65" FontSize="11" RenderTransformOrigin="0.5,0.5" Width="69.367" Height="17"/>
                                <ComboBox x:Name="comboPayOff"  Height="24" Width="115" Canvas.Left="110" Canvas.Top="81" SelectionChanged="event_ComboPayoff_SelectionChange"/>
                                <Button x:Name="btnTablaFixing" FontSize="9" Content="Tabla Fixing" Height="24" Width="83" Canvas.Left="232" Canvas.Top="123" Click="event_btnTablaFixing_Click"/>

                                <!--Ejercicio -->
                                <TextBlock Text="Ejercicio" Canvas.Left="236" Canvas.Top="65" FontSize="11" Width="71" Height="16"/>
                                <!-- alanrevisar este tiene el ancho nuevo mas grande
                                <ComboBox  x:Name="comboEjercicio" Height="24" Width="90" Canvas.Left="230" Canvas.Top="81" SelectedIndex="0" IsEnabled="False"  >
                                -->

                                <ComboBox  x:Name="comboEjercicio" Height="24" Width="85" Canvas.Left="230" Canvas.Top="81" SelectedIndex="0" IsEnabled="False"  >
                                    <ComboBoxItem Content="Europea"/>
                                    <ComboBoxItem Content="Bermuda"/>
                                    <ComboBoxItem Content="Americana"/>
                                </ComboBox>


                                <Border BorderThickness="1"  Background="AliceBlue"  BorderBrush="DarkGray" Canvas.Left="20" Canvas.Top="29" Width="295" Height="35" CornerRadius="5" />
                                <Canvas x:Name="CanvasOpionesContratoFront" Canvas.Top="35"  Height="120" Width="290" Canvas.Left="24">
                                    <!--Expander Opciones-->
                                    <MSControls:Expander  x:Name="expanderOpciones"  Canvas.Left="5" Canvas.Top="0" IsExpanded="False" Header="Opciones"  ExpandDirection="Down" MaxHeight="120" VerticalAlignment="Top" Width="195"  >
                                        <ContentControl Background="#DDDDDD" >
                                            <Border Background="#DDDDDD" BorderThickness="1" BorderBrush="Black" CornerRadius="0">
                                                <ScrollViewer HorizontalScrollBarVisibility="Disabled"  VerticalScrollBarVisibility="Visible">
                                                    <StackPanel x:Name="stackOpciones" Orientation="Vertical"  Background="Transparent" ScrollViewer.VerticalScrollBarVisibility="Auto" >
                                                        <RadioButton x:Name="radioOpcCall"  Content="Call" Checked="radioButton_Checked" />
                                                        <RadioButton x:Name="radioOpcPut" Content="Put" Checked="radioButton_Checked" />
                                                        <LiquidMenu:MenuDivider/>
                                                    </StackPanel>
                                                </ScrollViewer>
                                            </Border>
                                        </ContentControl>
                                    </MSControls:Expander>

                                    <!-- CheckBox Vega Weighted-->
                                    <CheckBox x:Name="checkBoxVegaWeighted" Content="Vega Weighted" Canvas.Left="100" Canvas.Top="4"  Visibility="Collapsed" Click="event_checkBoxVegaWeighted_Clicked" />
                                    <!-- Componentes-->
                                    <Button x:Name="btnComponentes" Canvas.Top="0" Canvas.Left="210" FontSize="9" Content="Componentes" Click="event_btnComponentes_Click" Height="24" Width="77"  VerticalAlignment="Top"  />

                                </Canvas>


                                <!-- STRIP ASIATICO -->
                                <Border BorderThickness="1"  Background="AliceBlue"  BorderBrush="DarkGray" Canvas.Left="20" Canvas.Top="155" Width="295" Height="35" CornerRadius="5" />
                                <Canvas x:Name="CanvasStrip" Canvas.Top="160"  Height="30" Width="290" Canvas.Left="24">

                                    <!-- CheckBox Asociado a STRIP-->
                                    <CheckBox x:Name="checkboxAsociadoStrip" FontSize="11" Canvas.Top="5" Canvas.Left="10"  Content="Asociado a un Strip" Height="20" Width="132"  VerticalAlignment="Top"  IsEnabled="False" />

                                    <!-- Botón STRIP-->
                                    <Button x:Name="btnStrip" Canvas.Top="0" Canvas.Left="210"  Content="Strip" Click="event_btnStrip_Click" Height="24" Width="70"  VerticalAlignment="Top" IsEnabled="False"/>

                                </Canvas>
                                <!-- /STRIP ASIATICO -->


                            </Canvas>
                        </Border>

                        <!---/ Definicion Opciones-->

                        <!--Definicion Nocionales-->
                        <Border Height="197" Width="345" BorderThickness="1"  Background="AliceBlue"  BorderBrush="DarkGray" Canvas.Left="350" Canvas.Top="82" CornerRadius="0">
                            <Canvas x:Name="CanvasNocional" >
                                <!--Radios Compensacion / Entrega_Fisica-->
                                <RadioButton x:Name="radioEntregaFisica" Content="Entrega Física" IsChecked="true" Canvas.Left="20" Canvas.Top="15" Checked="event_radioEntregaFisicaChecked"/>
                                <RadioButton x:Name="radioCompensacion" Content="Compensación" Canvas.Left="130" Canvas.Top="15" Checked="event_radioCompensacionChecked"/>

                                <!--Nocional-->
                                <TextBlock x:Name="txtNocional1_Flag" Text="Nocional" Width="50" Height="24" Canvas.Left="20" Canvas.Top="38"/>
                                <TextBox x:Name="txtNocional" Width="185" Height="24" Canvas.Left="20" Canvas.Top="57" KeyDown="event_KeyDown_DecimalText" GotFocus="event_GotFocus_DecimalText" TextChanged="event_TextChanged_DecimalText" LostFocus="event_LostFocus_DecimalText" TextAlignment="Right" />

                                <!--Nocional Strangle-->
                                <TextBlock Text="Nocional Strangle" x:Name="NocionalStrangle_flag"  Width="142.476" Height="20" Canvas.Left="20" Canvas.Top="118" Visibility="Collapsed" />
                                <TextBox x:Name="txtNocionalStrangle" Width="185" Height="24" Canvas.Left="20" Canvas.Top="144" TextAlignment="Right" Visibility="Collapsed" KeyDown="event_KeyDown_DecimalText" GotFocus="event_GotFocus_DecimalText" TextChanged="event_TextChanged_DecimalText" LostFocus="event_LostFocus_DecimalText" />

                                <!--Nocional Contra Moneda-->
                                <TextBlock x:Name="NocionalContraMoneda_flag"  Text="Nocional Contra Moneda" Width="142.476" Height="20" Canvas.Left="20" Canvas.Top="98" Visibility="Visible"/>
                                <TextBox x:Name="txtNocionalContraMoneda" GotFocus="event_GotFocus_DecimalText" IsReadOnly="True" Width="185" Height="24" Canvas.Left="20" Canvas.Top="118" TextAlignment="Right" Visibility="Visible"/>

                                <Border BorderBrush="DarkGray" BorderThickness="1" Width="58" Height="24" Canvas.Left="209" Canvas.Top="58" Background="#FFDEDEDE" >
                                    <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
                                        <TextBlock x:Name="txtMonedaNocional" Text="USD"/>
                                    </StackPanel>
                                </Border>

                                <Border BorderBrush="DarkGray" BorderThickness="1" Width="58" Height="24" Canvas.Left="209" Canvas.Top="118" Background="#FFDEDEDE" >
                                    <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
                                        <TextBlock x:Name="txtMonedaNocionalContraMoneda" Text="CLP"/>
                                    </StackPanel>
                                </Border>
                                <!-- Check Box Linea Puntual Control Linea Credito Tesoreria -->
                                <CheckBox Canvas.Left="20" Canvas.Top="163" Content="Linea Puntual" FontSize="11" Height="20" IsEnabled="True" Name="checkboxLineaPuntual" Width="132" />
                            </Canvas>
                        </Border>
                        <!-- /Definicion Nocionales-->

