                    <Canvas x:Name="PrincipalCanvas" Background="White" Width="700" Height="850" >

                        <Grid Background="#FF3A7ECB" Height="24" Width="685" Canvas.Left="10" Canvas.Top="7">
                            <TextBlock VerticalAlignment="Center" Grid.Column="1" Margin="10,0,0,0" Foreground="#FFFFFFFF" Text="Contrato" />
                        </Grid>


                        <!-- Fecha Valoracion-->
