                        <TextBlock Text="  Fecha Valoración" FontSize="10" Canvas.Top="36.5" Canvas.Left="20" />
                        <SisWinControls:DatePicker x:Name="datePiker_DateProccess" SelectedDateChanged="event_datePiker_DateProccess_SelectedChange"   Height="24" Width="150" Canvas.Top="50.5" Canvas.Left="25"/>
                        <Canvas Name="CanvasCoverDatePicker" Height="28" Width="160" Canvas.Top="48" Canvas.Left="20" Background="Transparent"/>

                        <TextBlock Text="  Set de Precios" FontSize="10" Canvas.Top="36.5" Canvas.Left="200" />

                        <Border Height="24" Width="100" BorderBrush="#FF97A1AB" Background="White" BorderThickness="1,1,1,1" RenderTransformOrigin="7.5,0.37" Canvas.Left="200" Canvas.Top="51.5">
                            <TextBlock x:Name="txtSetdePrecios_Pricing" Text="Riesgo" Height="24" Width="100" Margin="0,3,0,0"  TextAlignment="Center" TextWrapping="Wrap" RenderTransformOrigin="7.792,0.661" />
                        </Border>

                        <!-- Glosa Descriptiva Fwd Acotados-->
                        <Border Height="51" Width="345" BorderThickness="1" Background="AliceBlue" BorderBrush="#FF97A1AB" Canvas.Left="350" CornerRadius="0" Canvas.Top="31" />
                        <TextBlock Text="Glosa" FontSize="10" Canvas.Top="36.5" Canvas.Left="380" />
                        <TextBox x:Name="txtGlosaPricing" Width="300" Height="24" Canvas.Left="370" Canvas.Top="51.5" IsReadOnly="True" MaxLength="80" />

                        <!-- Definicion Opcion-->
